<div class="col-sm-12 col-md-3 col-lg-4">
  <% trainings.each do |training| %>
    <table>
      <tbody>
        <tr>
          <th>日付</th>
            <td><%= training.created_at.to_s(:datetime_jp) %></td>
        </tr>
        <tr>
          <th>ジム</th>
            <td><%= training.gym.name %></td>
        </tr>
        <tr>
          <th>partner</th>
            <td><%= training.partner %></td>
        </tr>
        <tr>
          <th>部位1</th>
            <td><%= training.part1 %></td>
        </tr>
        <tr>
          <th>部位2</th>
            <td><%= training.part2 %></td>
        </tr>
        <tr>
          <th>部位3</th>
            <td><%= training.part3 %></td>
        </tr>
        <tr>
          <th>option</th>
            <td>
              <% if current_user == training.user %>
                <%= link_to "削除", training, method: :delete, data: { confirm: "You sure?" }%>
              <% end %>
              <% if current_user == training.user %>
                <p><%= link_to "編集", edit_training_path(training)%>
              <% end %>
            </td>
        </tr>
      </tbody>
    </table>
  <% end %>
  <%= paginate trainings %>
</div>